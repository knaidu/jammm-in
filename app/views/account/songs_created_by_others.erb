<%= render_state({
  :url => "/account/published_songs", 
  :context_menu => "/partial/account/context_menu",
  :right_panel => "/partial/account/right_panel",
  :img => "/images/navigation/song.png"
}) %>

<%= vspace 10 %>
<%= page_header ["Account", "Songs", "Songs created by others"] %>
<%= vspace 20 %>

<%= partial("common/songs", :locals => {:songs => @songs}) %>

<% if @songs.count == 0 %>
  <div class="info-box">None of your jams/collaborations have been used in other collaborations.</div>
<% end %>