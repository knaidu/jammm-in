<%= render_state({
  :url => "/account/unpublished_songs", 
  :context_menu => "/partial/account/context_menu",
  :right_panel => "/partial/account/right_panel",
  :img => "/images/navigation/song.png"
}) %>

<%= vspace 10 %>
<%= page_header ["Account", "Collaborations", "Unpublished Collaborations"] %>
<%= vspace 20 %>

<%= partial("common/songs", :locals => {:songs => @songs}) %>

<% if @songs.count == 0 %>
  <div class="info-box">You have no unpublished collaborations.</div>
<% end %>