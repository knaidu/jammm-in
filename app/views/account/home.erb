<%= render_state({
  :name => "Home", 
  :description => "My Home Page", 
  :url => "/account/home", 
  :context_menu => "/partial/account/context_menu",
  :right_panel => "/partial/account/right_panel"
}) %>

<%= vspace 10 %>
<div class="page-header">Account <span class="slash">/</span> Home</div>
<%= vspace 20 %>

<% feeds = session_user?.feeds 100 %>
<div class='feeds'>
  <% feeds.each{|feed| %>
    <%= partial :"/account/feeds/#{feed.feed_type}", :locals => {:feed => feed} %>
  <% } %>
</div>