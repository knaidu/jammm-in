<% 
  jam = feed.dataObj.jam 
  users = feed.dataObj.users
%>
<div class='feed' containsonhoveritems=true>
  <div class='thumbnail link' <%= profile_link users[0] %>>
    <img src='<%= users[0].profile_picture_url %>'>
  </div>
  <div class='content'>
    <span class='bold link clickable' <%= profile_link users[0] %>><%= users[0].username %></span> liked
    <div class='entity'>
      <div class='entity-image'>
        <img src='<%= jam.image_url_big %>'>
      </div>
      <div class='entity-info'>
        <div class='bold link' <%= music_link jam %>><%= jam.name %></div>
        <%= vspace 10 %>
        <div>
          <div class='field-name'>Views:</div><%= jam.views %>
          <%= hspace 10 %>
          <div class='field-name'>Length:</div><%= jam.file_data.length.to_time_str rescue "-" %>
          <%= hspace 10 %>
          <div class='field-name'>Likes:</div><%= jam.liked_by.count %>
        </div>
      </div>
    </div>
    <div class='time'><%= feed.created_at.reference_string %>&nbsp;
      <div class="float-right"><%= render_mini_play_text(jam) %></div>
    </div>
  </div>
</div>