<% users = feed.dataObj.users %>
<div class='feed'>
  <div class='thumbnail link' <%= profile_link users[0] %>>
    <img src='<%= users[0].profile_picture_url %>'>
  </div>
  <div class='content'>
    <span class='bold link clickable' <%= profile_link users[0] %>><%= users[0].username %></span> is following
    <div class='entity'>
      <div class='entity-image link' <%= profile_link users[1] %>>
        <img src='<%= users[1].profile_picture_url %>'>
      </div>
      <div class='entity-info'>
        <div class='bold clickable link' <%= profile_link users[1] %>><%= users[1].username %></div>
        <%= vspace 10 %>
        <div>
          <div class='field-name'>Jams:</div><%= users[1].jams.count %>
          <%= hspace 10 %>
          <div class='field-name'>Collaborations:</div><%= users[1].songs.count %>
        </div>
      </div>
    </div>
    <div class='time'><%= feed.created_at.reference_string %></div>
  </div>
</div>