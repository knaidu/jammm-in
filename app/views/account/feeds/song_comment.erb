<% 
  song = feed.dataObj.song 
  comment = feed.dataObj.comment
%>
<div class='feed'>
  <div class='thumbnail'>
    <img src='<%= comment.user.profile_picture_url %>'>
  </div>
  <div class='content'>
    <div class='bold'><%= comment.user.username %></div><%= vspace %>
    "<%= feed.dataObj.comment.comment %>"
    <div class='entity'>
      <div class='entity-image'>
        <img src='/new-ui/collaborate.png'>
      </div>
      <div class='entity-info'>
        <div class='bold'><%= song.name %></div>
        <%= vspace 10 %>
        <div>
          <div class='field-name'>Views:</div><%= song.views %>
          <%= hspace 10 %>
          <div class='field-name'>Length:</div><%= song.file_data.length.to_time_str rescue "-" %>
          <%= hspace 10 %>
          <div class='field-name'>Likes:</div><%= song.liked_by.count %>
        </div>
      </div>
    </div>
    <div class='time'><%= feed.created_at.reference_string %></div>
  </div>
</div>