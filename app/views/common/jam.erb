<div class='w100 pad1'>
  <span class='left-float pad-right-10'><%= add_icon 'jam' %></span>
  <div class='left-float'>
    <span class='bold float-left'><a href='/jam/<%= jam.id %>'><%= jam.name %></a>&nbsp;</span><span class='float-left'>[length]</span><br>
    <%= add_field_name 'Jammmed by' %> <%= partial(:'common/list_artists', :locals => {:artists => jam.artists}) %><br>
    <%= add_field_name 'Likes' %> <%= jam.liked_by.size %>
  </div>
  <span class='float-right'>Add, Play, Maximize</span>
</div>

