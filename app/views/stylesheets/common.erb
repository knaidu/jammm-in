<%  
	default_color = "#cc0000"
	default_dark_color = "#800000"
	default_round = "-moz-border-radius: 5px; -webkit-border-radius: 5px; border-radius: 5px;"
	round_2 = "-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px;"
	light_gray  = "#eee9e9"
	font_family = "Lucida Grande, Verdana, Arial,Helvetica,sans-serif"
	left_panel_width = "215px";
	right_panel_width = "270px";
	top_bar_height = "44px";
	footer_height = "30px";
	actions_doc_height = "35px";
	testing_gray = "#eeeeee";
  shadow = "-moz-box-shadow: 2px 2px 5px #888;
  -webkit-box-shadow: 2px 2px 5px #888;
  box-shadow: 2px 2px 5px #888;"
  shadow_right = "-moz-box-shadow: 2px 2px 25px #888;
  -webkit-box-shadow: 2px 2px 5px #888;
  box-shadow: 2px 2px 5px #888;"
  row_bottom_border = "border-bottom: 1px solid #ccc;"
%>

/* DEFAULTS */

.default-link-color {color: #990000;}

.round-2 {<%= round_2 %>}
.shadow {<%= shadow %>}
.round {<%= default_round %>}

/* HTML, BODY */
html, body {
  margin:0; 
  padding:0; 
  width:100%; 
  height:100%; 
  overflow: auto;
  font-family: <%= font_family %>; 
  font-size: 12px;
}

TEXTAREA, INPUT[type='text'], INPUT[type='password'], INPUT[type='file'], SELECT {width: 200px}
TEXTAREA {height: 60px}

INPUT[type='button'], INPUT[type='submit'] {border: 1px solid #bbb; background-image: url("/new-ui/button-bg.png"); padding: 6px 12px; color: #444; font-weight: bold}
INPUT[type='button'].down, INPUT[type='submit'].down {border: 1px solid #888}

/* GENERAL */
.clear-both {clear: both}
.position-absolute {position: absolute}
.position-relative {position: relative}
.float-left {float: left}
.float-right {float: right}
.w50 {width: 50px}
.w100 {width: 100px}
.h30 {height: 30px}
.s11 {font-size: 11px}
.hide {display: none}
.display-inline-block {display: inline-block}
.align-right {text-align: right}
.link {cursor: pointer}
.red {color: <%= default_color %>}
.red.important {color: <%= default_color %> !important}
.bold {font-weight: bold}
.gray {color: #888}
.overflow-hidden {overflow: hidden}
.qoute {font-size: 16px; color: #888}
.small-field {font-size: 11px; color: #888; display: inline-block; margin-right: 20px}
.slash {color: #bbb; font-size: 16px}
.simple-field {display: inline-block; color: #888}
.onhover {background-color: #FBFFF0}
.clickable {cursor: pointer}
.underline {text-decoration: underline}
.opacity-02{opacity: 0.2}
.line {border-bottom: 1px solid #888; height: 2px}
.no-change {color: inherit; text-decoration: none; }
.hover-underline {}
.hover-underline:hover {text-decoration: underline}

IMG {border: none}

IFRAME {border-width: 0px; margin-left: -5px; width: 100%; height: 200px;}

.top-bar {
  height: <%= top_bar_height %>;
  background-image: url("/new-ui/header-bg.png");
  color: white;
  position: fixed;
  width: 100%;
  z-index: 3;
}

.top-bar .logo {position: absolute; left: 35px; width: <%= left_panel_width %>; height: 100%; }
.top-bar .search {position: absolute; right: 0px; width: <%= right_panel_width %>; height: 100%; padding: 5px; text-align: right}
.top-bar .search INPUT {width: 100%; visibility: hidden; width: 0px}
.top-bar .menu {position: absolute; right: 0px; color: white; font-size: 14px; padding: 5px 5px 5px 20px}
.top-bar .menu .item {display: inline-block; margin-right: 10px; cursor: pointer}
.top-bar .menu .item .text {padding: 5px 5px; font-weight: bold;}

.middle-bar {
  margin-top: <%= top_bar_height %>;
  position: fixed;
  overflow: auto;
}

.middle-bar .menu-panel {position: fixed; border-right: 0px solid <%= testing_gray %>; background-color: white; width: <%= left_panel_width %>; z-index: 2;}
.middle-bar .tips-panel {position: fixed; right: 0px; width: <%= right_panel_width %>; border-left: 0px solid grey; min-height: 300px; color: #777; font-size: 11px; padding: 10px 0px 0px 0px}
.middle-bar .content-panel {
  position: relative; 
  margin-left: <%= left_panel_width %>; 
  margin-right: <%= ((right_panel_width).px + 20).px %>; 
  z-index: 1; 
  padding: 0px 0px 0px 0px;
  overflow: hidden;
}

.bottom-bar {position: fixed; bottom: 0px; width: 100%; height: <%= footer_height %>; background-color: #ebebeb; border: 1px solid #aaa; z-index: 4}
.bottom-bar .items {
  position: absolute;
  left: 0px;
  color: #666;
  font-size: 11px;
  width: 400px;
  padding: 8px 2px;
}
.bottom-bar .items .item {display: inline-block; margin-left: 4px; cursor: pointer}

.menu-panel .navigation-bar {width: 102%; color: #666666; height: 70px; display: none; cursor: pointer}
.menu-panel .navigation-bar .icon {position: absolute;}
.menu-panel .navigation-bar .images {position: absolute; left: 23px; width: 32px; margin-top: 10px; width: 170px}
.menu-panel .navigation-bar .images .image {
  position: absolute; 
  height: 48px; 
  width: 48px; 
  top: 0px; 
  border: 0px solid white; 
  overflow: hidden
}
.menu-panel .navigation-bar .images .image IMG {height: 100%; width: 100%; <%= default_round %>; }
.menu-panel .navigation-bar .images .image.highlight {background-color: #ddd; <%= default_round %>; border: 0px solid #bbb;}
.menu-panel .navigation-bar .content {position: relative; height: 60px; margin-left: 80px; margin-top: 0px; padding-right: 25px; overflow: hidden;}
.menu-panel .navigation-bar .content .name {font-weight: normal; padding-top: 8px}
.menu-panel .navigation-bar .content .description {font-weight: normal; font-size: 10px}

.context-menu {margin-left: 0px; margin-top: 10px; cursor: default !important}
.context-menu .highlight {cursor: pointer}
.context-menu .item {position: relative; border: 0px solid <%= testing_gray %>;; color: #555}
.context-menu .container.onhover {
  background-color: #EBEBEB;
  color: #555;
  <%= round_2 %>
}
.context-menu .item .icon {position: absolute; left: 10px; top: 5px; color: #696969; height: 24px; overflow: hidden; height: 35px}
.context-menu .item .text {position: relative; padding: 10px 0px 10px 0px; margin-left: 42px;}
.context-menu .item.sub {font-size: 11px; height: 20px; padding: 8px 0px 3px 0px}
.context-menu .item.sub .text{padding-top: 0px}
.context-menu .partition {border-top: 1px dotted #ebebeb; margin: 10px 0px; width: "80%"}

.actions-doc {
  position: fixed; 
  width: <%= (right_panel_width.px - 10).px %>; 
  right: 5px; 
}
.actions-doc .contents {margin-left: 3px; overflow: auto; height: <%= footer_height %>}
.actions-doc .contents .item {float: right; height: 20px; overflow: auto; margin-right: 13px; height: 100%; cursor: pointer}
.actions-doc .contents .item .text {margin-top: 5px; padding: 4px 10px; <%= round_2 %>; background-color: #bbb; color: #555; font-size: 11px}
.actions-doc .contents .item .text.login {background-color: #cc0000; color: white}
.actions-doc .contents .item .icon {margin-top: 3px}
.player-container {
  display: none; 
  position: fixed; 
  bottom: <%= (footer_height.px + 1).px %>; 
  width: <%= (right_panel_width.px - 10).px %>; 
  right: 0px;
  height: 20px;
  border: 1px solid #aaa;
  overflow: hidden;
  background-color: #ebebeb;
}
.player-container .header {color: white; background-color: #888; font-weight: bold; padding: 3px 5px 5px 5px; font-size: 10px; cursor: pointer}
.player-container .header .minimize {position: absolute; right: 5px; font-size: 15px; top: 0px; font-weight: bold}
.player .buffer {position: absolute; top: 0px; background-color: #888}
.player .seek {
  top: 0px;
  background-color: #bbb;
  height: 100%;
  font-size: 10px;
  position: absolute; 
  width: 0px;
}
.player .play-icon {position: absolute; top: -1px; z-index: 3}
.player .text {padding: 3px 0px 0px 24px; top: 0px; position: absolute; z-index: 3; color: #555; font-weight: bold; font-size: 11px}
.player .item {margin-bottom: 10px; border-bottom: 1px solid #ededed; color: #aaa}
.player .item-onhover {color: #666666}

.page-header {font-size: 14px; font-weight: bold}

.list {position: relative;}
.list .row {position: relative; padding: 10px 0px; min-height: 60px; overflow: auto; padding-left: 5px; border-bottom: 1px solid #ddd; cursor: pointer}
.list .row .forward-icon {position: absolute; right: 20px; margin-top: 15px}
.list .row .image {position: absolute; width: 54px; height: 54px; border: 0px solid #dcdcdc; padding: 1px}
.list .row .content {position: relative;}
.list .row .content .col1 {position: absolute; margin-left: 70px; width: 200px}
.list .row .content .col2 {position: absolute; margin-left: 300px;}
.list .row .content .col3 {position: absolute; margin-left: 490px; color: black; font-size: 10px; width: 200px}
.list .row .content .name {overflow: hidden; color: black; font-weight: bold; cursor: pointer; margin-bottom: 3px}
.list .row .content .artist {overflow: hidden; font-size: 10px}
.list .row.onhover {background-color: #FBFFF0}
/*.list .row.onhover {background-color: #f2f2f2} */

.feed {position: relative; min-height: 64px; padding-bottom: 20px}
.feed .profile-picture {position: absolute; left: 0px; height: 48px; width: 48px;}
.feed .profile-picture IMG {height: 100%; width: 100%}
.feed .content {position: inherit; margin-left: 64px;}
.feed .content .title {color: grey; font-size: 13px}
.feed .content .title .name {font-weight: bold}


.state {visibility: hidden; height: 0px}

.onhover-border-shadow {border: 1px solid #888888; <%= shadow %>}

.show-all {color: #777; font-size: 11px; position: absolute; bottom: 0px; height: 15px; background-color: #ccc; padding: 3px; width: 100%; text-align: center; cursor: pointer}
.show-all:hover {background-color: #bbb}

.playlist {position: relative; padding: 5px}
.playlist .item {position: relative; min-height: 50px; padding-bottom: 5px;}
.playlist .item .image {position: absolute; width: 48px; opacity: 0.8}
.playlist .item .contents {position: relative; margin-left: 54px; color: inherit; font-weight: bold; font-size: 11px}
.playlist .item .contents .name {margin-bottom: 2px}
.playlist .item .contents .artist {font-weight: normal; font-size: 10px}

.notifications {position: relative; padding: 5px}
.notifications .item {position: relative; min-height: 20px; padding-bottom: 5px;}
.notifications .item .image {position: absolute; width: 16px; opacity: 0.8; overflow: hidden}
.notifications .item .contents {position: relative; margin-left: 30px; color: inherit; font-weight: normal; font-size: 10px}

.messages {position: relative; padding: 5px}
.messages .item {position: relative; min-height: 50px; padding-bottom: 5px; cursor: pointer} 
.messages .item .image {position: absolute; width: 48px; opacity: 0.8}
.messages .item .contents {position: relative; margin-left: 54px; color: inherit; font-weight: normal; }
.messages .item .contents .name {margin-bottom: 2px; font-size: 11px; font-weight: bold}
.messages .item .contents .body {font-size: 10px}

.content-panel .header {font-size: 20px; font-weight: normal; color: #444}
.content-panel .field {display: inline-block; color: #888; width: 200px;}
.content-panel .field.w50 {width: 50px}

.single-jam.jam {position: relative; height: 96px; overflow: hidden;}
.single-jam.jam .forward-icon {position: absolute; right: 20px; margin-top: 25px}
.single-jam.jam .image {position: absolute; width: 78px; height: 78px; padding: 1px}
.single-jam.jam .content {position: relative; margin-left: 96px}
.single-jam.jam .content .name {overflow: hidden; color: black; font-weight: bold; cursor: pointer; margin-bottom: 3px}
.single-jam.jam .content .artist {overflow: hidden; font-size: 10px}
.single-jam.jam .content .play-waveform {position: relative}
.single-jam.jam .content .play-waveform .play {cursor: pointer; position: absolute; width: 50px; left: -5px; margin-top: 3px}
.single-jam.jam .content .play-waveform .waveform {padding-top: 0px; position: relative; margin-left: 50px; height: 50px;}

.jam-page.songs {}
.jam-page.songs .song {position: relative; height: 50px; padding: 5px 0px 10px 0px; font-size: 11px}
.jam-page.songs .song .play {position: absolute; width: 50px}
.jam-page.songs .song .name {position: absolute; left: 50px}
.jam-page.songs .song .content {position: relative; margin-left: 200px}


.section-header {background-color: #EBEBEB; font-size: 11px; color: #333; font-weight: bold; padding: 2px 0px 2px 5px; border-bottom: 1px solid #ccc; margin-bottom: 10px}

.comments {position: relative;}
.comments .comment {position: inherit; min-height: 30px; margin-bottom: 20px; font-size: 11px}
.comments .comment .image {position: absolute; width: 36px;}
.comments .comment .body {position: relative; margin-left: 36px; max-width: 600px}
.comments .comment .body .actions {position: absolute; right: 0px; width: 100px}
.comments .comment .body .username {font-size: 11px; font-weight: bold}

.song-page.song {position: relative; height: 96px; overflow: auto;}
.song-page.song .forward-icon {position: absolute; right: 20px; margin-top: 25px}
.song-page.song .image {position: absolute; width: 78px; height: 78px; padding: 1px}
.song-page.song .content {position: relative; overflow: auto; min-height: 78px; margin-left: 96px;}
.song-page.song .content .name {overflow: hidden; color: black; font-weight: bold; cursor: pointer; margin-bottom: 3px}
.song-page.song .content .artist {overflow: hidden; font-size: 10px}
.song-page.song .content .play-waveform {position: relative}
.song-page.song .content .play-waveform .play {position: absolute; width: 50px; left: -5px}
.song-page.song .content .play-waveform .waveform {padding-top: 0px; position: relative; margin-left: 50px; height: 50px;}
.song-page.song .content .play-waveform .waveform IMG {height: 100%; width: 100%}

.song-page.jams {position: relative}
.song-page.jams .jam {position: relative; height: 50px; margin-bottom: 10px}
.song-page.jams .jam .play-icon {position: absolute; width: 50px}
.song-page.jams .jam .info {position: absolute; left: 52px; top: 5px; width: 140px}
.song-page.jams .jam .info .name {font-size: 11px; font-weight: bold}
.song-page.jams .jam .info .artist {font-size: 10px; font-weight: normal}
.song-page.jams .jam .info .instrument {font-size: 10px; font-weight: normal}
.song-page.jams .jam .spectrogram {position: relative; margin-left: 200px; height: 50px;}
.song-page.jams .jam .spectrogram IMG {height: 100%; width: 100%}

/* WAITING DIALOG */
.waiting-dialog {position: fixed; top: 0px; left: 0px; width: 100%; height: 100%; background-color: #666; opacity: 0.5; z-index: 5;}

#basic-modal-content {display:none;}

/* Overlay */
#simplemodal-overlay {background-color:#000; cursor:wait;}

/* Container */
#simplemodal-container {color:#bbb; background-color: white; border:4px solid #444; padding:12px;}
.simplemodal-data {padding:8px; color: #333; overflow: auto;}
.simplemodal-data-iframe {padding:8px; color: #333; overflow: auto;} /* MUST BE IDENTICAL TO .simplemodal-data */

#simplemodal-container code {background:#141414; border-left:3px solid #65B43D; color:#bbb; display:block; font-size:12px; margin-bottom:12px; padding:4px 6px 6px;}
#simplemodal-container a {color:#ddd;}
#simplemodal-container a.modalCloseImg {background:url(/images/x.png) no-repeat; width:28px; height:29px; display:inline; z-index:3200; position:absolute; top:-15px; right:-16px; cursor:pointer;}
#simplemodal-container h3 {color:#84b8d9;}

/* DATA */
.simplemodal-data .header {font-size: 16px; color: #000; border-bottom: 1px solid #bbb; padding-bottom: 5px}
.simplemodal-data .field {display: inline-block; color: #666; width: 130px}

.simplemodal-data-iframe {padding:8px; color: #333; overflow: auto;} /* MUST BE IDENTICAL TO .simplemodal-data */
.simplemodal-data-iframe .header {font-size: 16px; color: #000; border-bottom: 1px solid #bbb; padding-bottom: 5px}
.simplemodal-data-iframe .field {display: inline-block; color: #666; width: 130px}

.editable-genres {width: 300px;}
.editable-genres .editable-genre {display: inline-block; position: relative; min-height: 20px; margin-right: 20px}
.editable-genres .editable-genre .action {position: absolute; right: 0px}
.editable-genres .editable-genre .text {padding-right:20px;}

.song-manage-page.jams{position: relative}
.song-manage-page.jams .jam {position: relative; min-height: 60px; border-bottom: 1px solid #dcdcdc; padding: 10px 0px 10px 0px}
.song-manage-page.jams .jam .checkbox{position: absolute; width: 20px; margin-top: 15px}
.song-manage-page.jams .jam .play {position: absolute; width: 50px; margin-left: 25px}
.song-manage-page.jams .jam .instrument {position: absolute; width: 50px; margin-left: 75px}
.song-manage-page.jams .jam .info {position: relative; margin-left: 30px}
.song-manage-page.jams .jam .info .name {display: inline-block; font-size: 11px; font-weight: bold;}
.song-manage-page.jams .jam .info .artist {display: inline-block; font-size: 10px}
.song-manage-page.jams .jam .info .active {margin-left: 20px; display: inline-block; font-size: 10px; color: green}
.song-manage-page.jams .jam .waveform {position: relative; margin-left: 200px; margin-right: 100px; height: 50px;}
.song-manage-page.jams .jam .volume {position: absolute; margin-left: 120px; height: inherit}
.song-manage-page.jams .jam .actions {position: absolute; right: 0px; font-size: 11px}
.song-manage-page.jams .jam .volume .icon {height: 24px; width: 24px; padding: 5px; margin-left: -5px}
.song-manage-page.jams .jam .volume .slider {width: 65px; height: 20px; margin-top: -3px}
.song-manage-page.jams .jam.selected {background-color: #e7e7e7}

.jquery-slider {font-size: 8px !important}

.messages {position: relative}
.messages .message {position: relative; min-height: 100px}
.messages .message .image {position: absolute; width: 80px}
.messages .message .content {position: relative; margin-left: 80px;}
.messages .message .content .username {font-weight: bold; font-size: 11px}
.messages .message .content .body {}

.waveform {min-height: 50px; background-color: #bbb;}
.waveform .buffer {position: absolute; width: 0px; height: 100%; background-color: #888; }
.waveform .seek {position: absolute; width: 0px; background-color: #cc0000; height: 100%}
.waveform .seek .waveform-image {border: none}

.tip {color: #777; border: 1px solid #e8e8e8; background-color: #f6f6f6; position: relative; font-size: 11px; padding: 10px}
.tip .image {position: absolute; right: -3px; top: -10px; height: 36px}

.memory-usage {position: relative; height: 15px; border: 1px solid #bbb}
.memory-usage .fill {display: inline-block; height: inherit; background-color: #ebebeb}

.more {text-align: right; color: #777; font-size: 11px}

.feeds {position: relative}
.feeds .feed {position: relative; border-bottom: 1px solid #f0f0f0; padding: 10px 0px 10px 0px; font-size: 11px}
.feeds .feed .thumbnail {position: absolute; top: 12px; height: 48px; width: 48px;}
.feeds .feed .thumbnail IMG {height: 100%; width: 100%}
.feeds .feed .content {position: relative; margin-left: 60px;}
.feeds .feed .content .entity {position: relative; margin-top: 10px; margin-bottom: 10px}
.feeds .feed .content .entity-image {position: absolute; height: 42px; width: 42px}
.feeds .feed .content .entity-image IMG {height: 100%; width: 100%}
.feeds .feed .content .entity-info {position: relative; margin-left: 54px}
.feeds .feed .content .entity-info .field-name{color: #888; display: inline-block; margin-right: 5px; }
.feeds .feed .content .time {margin-top: 15px; color: #aaa; font-size: 10px}

.user-profile {position: relative; min-height: 80px; padding: 10px; background-color: #ebebeb; border: 1px solid #ccc; font-size: 11px}
.user-profile .profile-picture {position: absolute; height: 78px; width: 78px; border: 1px solid #999; background-color: white}
.user-profile .profile-picture IMG {height: 100%; width: 100%}
.user-profile .content {margin-left: 90px;}
.user-profile .content .info {margin-top: 0px}

.tabs {position: relative; font-size: 0px; height: 35px}
.tabs .padding-left {float: right; width: 0px; border-bottom: 1px solid #aaa; height: inherit; font-size: 11px;}
.tabs .padding-right {float: right; width: 250px; border-bottom: 1px solid #aaa; height: inherit; font-size: 11px;}
.tabs .tab {float: right; min-width: 50px; border-bottom: 1px solid #aaa; height: inherit;  font-size: 11px;  cursor: pointer}
.tabs .tab:hover {background-color: #f5f5f5}
.tabs .tab.selected {border: 1px solid #aaa !important; border-bottom-width: 0px !important; background-color: #ebebeb; font-weight: bold}
.tabs .tab .content {float: left; padding: 5px 0px 0px 5px}
.tabs .tab .content .text {position: relative; margin-left: 25px; padding: 5px 10px 5px 5px}
.tabs .tab .content .image {position: absolute; height: 24px; width: 24px; margin-top: 5px; margin-left: 3px}

.group-profile {position: relative; min-height: 100px}
.group-profile .profile-picture {position: absolute; height: 78px; width: 78px}
.group-profile .profile-picture IMG {height: 100%; width: 100%}
.group-profile .content {margin-left: 84px;}
.group-profile .content .info {margin-top: 0px}

.manage-group-users {position: relative}
.manage-group-users .user {min-height: 40px; <%= row_bottom_border %>; padding: 10px 0px}
.manage-group-users .user .image {position: absolute; height: 40px; width: 40px}
.manage-group-users .user .image IMG {height: 100%; width: 100%}
.manage-group-users .user .content {margin-left: 50px}
.manage-group-users .user .actions {position: absolute; right: 0px; padding-top: 3px}

.users {}
.users .user {position: relative; min-height: 54px; padding: 10px 0px; <%= row_bottom_border %>; cursor: pointer}
.users .user .profile-picture {position: absolute; height: 54px; width: 54px}
.users .user .profile-picture IMG {height: 100%; width: 100%}
.users .user .actions {position: absolute; right: 0px}
.users .user .content {margin-left: 60px}
.users .user .content .col1 {position: absolute; left: 60px}
.users .user .content .col2 {position: absolute; left: 300px}
.users .user .content .col3 {position: absolute; left: 480px}

.button {position: relative; display: inline-block; height: 28px; border: 1px solid #bbb; background-image: url("/new-ui/button-bg.png"); cursor: default;}
.button .icon {position: absolute; height: 16px; width: 16px; left: 8px; top: 7px}
.button .icon IMG {}
.button .text {margin-left: 30px; padding: 7px 12px 0px 0px; font-weight: bold; font-size: 11px}
.button.down {border: 1px solid #888}

.notifications-page {position: relative}
.notifications-page .notification {position: relative; margin-left: 10px; font-size: 11px; margin-bottom: 10px}

.error-response {color: #cc0000}

.overview.menu {}
.overview.menu .item{padding: 13px 15px; color: white; font-weight: bold; cursor: pointer; }
.overview.menu .what {background-color: #007a7a; border-bottom: 1px solid #bbb}
.overview.menu .why {background-color: #00a300}
.overview.menu .how {background-color: #cca900}
.overview.menu .gray {background-color: #ebebeb; color: black}

#overview-overlay {
  position: absolute; 
  border: px solid #777; 
  border: 1px solid #ccc;
  margin-left: 8px;
  padding: 10px 20px 10px 10px;
  border-top-width: 0px;
/*  background: rgba(50, 50, 50, 0.78); */
  background-color: #ebebeb;
  opacity: 0.78;
  height: 0px; 
  width: <%= (right_panel_width.px - 30).px %>; 
/*  display: none; */
  top: 0px; 
  left: 0px;
  margin-top: 0px;
  color: black;
}

#overview-overlay .overview-header {font-size: 16px; margin-bottom: 20px}
#overview-overlay .overview-footer {position: absolute; font-size: 14px; bottom: 10px; right: 10px; }
#overview-overlay .close {position: absolute; right: -15px; top: -15px; cursor: pointer}

.message-streams {}
.message-streams .message-stream {position: relative; min-height: 54px; padding: 10px 0px; <%= row_bottom_border %>; cursor: pointer}
.message-streams .message-stream .profile-picture {position: absolute; height: 54px; width: 54px; left: 5px}
.message-streams .message-stream.unread {background-color: #E0FCCA}
.message-streams .message-stream .profile-picture IMG {height: 100%; width: 100%}
.message-streams .message-stream .actions {position: absolute; right: 0px}
.message-streams .message-stream .content {margin-left: 70px}

.message-stream-messages {position: relative}
.message-stream-messages .message {position: relative}
.message-stream-messages .message .profile-picture {position: absolute; height: 54px; width: 54px}
.message-stream-messages .message .profile-picture IMG {height: 100%; width: 100%}
.message-stream-messages .message .body {margin-left: 64px;}

.user-messages {position: relative}
.user-messages .user-message {position: relative; min-height: 54px; margin-bottom: 10px; font-size: 11px}
.user-messages .user-message .actions {position: absolute; right: 0px; width: 100px}
.user-messages .user-message .profile-picture {position: absolute; height: 54px; width: 54px}
.user-messages .user-message .profile-picture IMG {height: 100%; width: 100%}
.user-messages .user-message .content {margin-left: 64px; margin-right: 100px}

.mini-play {position: relative; display: none; cursor: pointer}
.mini-play .text {float: left; margin-right: 3px; margin-top: 5px}
.mini-play .icon {float: left}

.info-box {background-color: #f5f5f5; padding: 10px; color: #444; font-size: 11px; border: 1px solid #ddd}

.unpublished-overlay {left: 80px; top: 15px; position:absolute; text-align: center; font-weight: bold; font-size: 11px}

.faq {margin-bottom: 20px}
.faq .question {margin-bottom: 5px; font-size: 12px; font-weight: bold}
.faq .answer {font-size: 11px}

.soundcloud-tracks {}
.soundcloud-tracks .soundcloud-track {position: relative; padding: 5px 0px; <%= row_bottom_border %>}
.soundcloud-tracks .soundcloud-track.selected {background-color: #f5f5f5}
.soundcloud-tracks .soundcloud-track .checkbox {position: absolute; left: 0px}
.soundcloud-tracks .soundcloud-track .contents {position: relative; margin-left: 30px}