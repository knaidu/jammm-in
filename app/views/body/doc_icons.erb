<div class='contents'>
  <% if not session_user? %>
    <div class="item" onclick='Doc.Login.show()'><div class="text">Login</div></div>
  <% else %>
    <div class="item"><div class="text">Logout</div></div>
    <div class="item"><div class="icon" onclick='Navigate.loadContent("/account")'><%= icon '/images/dock.png', 0, 24, true %></div></div>
    <div class="item"><div class="icon" onclick='Navigate.loadContent("/<%= session_user?.username %>")'><%= icon '/images/dock.png', 1, 24, true %></div></div>
    <div class="item"><div class="icon"><%= icon '/images/dock.png', 2, 24, true %></div></div>
    <div class="item"><div class="icon"><%= icon '/images/dock.png', 4, 24, true %></div></div>
    <div class="item"><div class="icon"><%= icon '/images/dock.png', 6, 24, true %></div></div>  
  <% end %>
</div>