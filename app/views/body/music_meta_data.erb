<% meta = @music_meta_data %>

<link rel="video_src" href="http://www.jammm.in/swf/new-player.swf?imageurl=http://jammm.in<%= meta[:image_src]%>&songtitle=<%= meta[:title] %>&url=http://www.jammm.in/file/<%= meta[:file] %>&artists=<%= meta[:artists]%>"/>

<meta name="title" content="<%= meta[:title] %>" />
<% if meta[:description] %>
  <meta name="description" content="<%= meta[:description] %>" />
<% end %>
<link rel="image_src" href="<%= meta[:image_src] %>">
<meta name="video_height" content="120" />
<meta name="video_width" content="400" />
<meta name="video_type" content="application/x-shockwave-flash" />
