<% 
  meta = @music_meta_data 
  full_image_src = "http://jammm.in#{meta[:image_src]}"
%>

<link rel="video_src" href="http://www.jammm.in/swf/new-player.swf?imageurl=<%= full_image_src %>&songtitle=<%= meta[:title] %>&url=http://www.jammm.in/file/<%= meta[:file] %>&artists=<%= meta[:artists]%>&pageurl=<%= meta[:page_url] %>"/>

<meta name="title" content="<%= meta[:title] %>" />
<% if meta[:description] %>
  <meta name="description" content="<%= meta[:description] %>" />
<% end %>
<link rel="image_src" href="<%= full_image_src %>">
<meta name="video_height" content="120" />
<meta name="video_width" content="460" />
<meta name="video_type" content="application/x-shockwave-flash" />
