<% 
	jam = fhlp_get_jam(feed) 
	users = fhlp_get_users(feed)
%>
<div>
	<span class='float-left'>
		<%= add_icon_with_pad %>
	</span>
	<% if not is_update %>
		Your jam <%= jam_link(jam) %> has been published.
	<% else %>
		Jam <%= jam_link(jam) %> by <%= list_artists(users) %> has been published
	<% end %>
</div>